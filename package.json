{
  "name": "hackernews-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "node deploy.sh"
  },
  "dependencies": {
    "chart.js": "^3.7.1",
    "core-js": "^3.8.3",
    "pretty-checkbox-vue": "^1.1.9",
    "randomcolor": "^0.6.2",
    "vue": "^3.2.13",
    "vue-chartjs": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.6.1",
    "execa": "latest"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "node_modules",
      "dist",
      "src/data/source"
    ],
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": 1
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
